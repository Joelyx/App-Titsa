a)
for $prov in /datos/proveedores/proveedor
where $prov/estado > 15
return data($prov/ciudad)

b)
count(/datos/proyectos/proyecto[ciudad='Londres'])

c)
for $proyecto in /datos/proyectos/proyecto[ciudad='Paris']
let $numproyecto := $proyecto/@numproyecto
where sum(/datos/suministros/suministra[numproyecto = $numproyecto]/cantidad) > 350
return data($proyecto/nombreproyecto)

